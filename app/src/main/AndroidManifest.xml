<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name="com.example.fundatecheroes2.App"
        android:theme="@style/Theme.FundatecHeroes2"
        tools:targetApi="31">
        <activity
            android:name=".character.view.NewCharacterActivity"
            android:exported="false"
            android:parentActivityName=".home.HomeActivity"/>
        <activity
            android:name=".navigation.BottomNavigationActivity"
            android:exported="false"
            android:parentActivityName=".home.HomeActivity"/>
        <activity
            android:name=".profile.view.ProfileActivity"
            android:exported="false"
            android:parentActivityName=".login.view.LoginActivity"/>
        <activity
            android:name=".home.HomeActivity"
            android:exported="false"
            android:parentActivityName=".login.view.LoginActivity" />
        <activity
            android:name=".login.view.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
    </application>

</manifest>